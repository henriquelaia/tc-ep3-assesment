M:
  states: [s, q1, q2, qA, qR]
  input_alphabet: [a]
  tape_alphabet_extra: [X, "Y", Z]
  start_state: s
  accept_state: qA
  reject_state: qR
  delta:
    s: # Write X at 0, Shift L
      a: [q1, X, L]
    q1: # At 0 (new), write Y, Shift L
      _: [q2, "Y", L]
    q2: # At 0 (new), write Z
      _: [qA, Z, S]

w: "a"
